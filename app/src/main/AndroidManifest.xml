        </activity>
        <activity android:name=".LayoutBasicsActivity" />
        <activity android:name=".FrameLayoutActivity">
        </activity>
        <activity android:name=".LinearLayoutActivity">
        </activity>
        <activity android:name=".LinearLayoutFormActivity">
        </activity>
        <activity android:name=".RelativeLayoutActivity">
        </activity>
        <activity android:name=".TableLayoutActivity">
        </activity>
        <activity android:name=".GridLayoutActivity">
        </activity>
    </application>

</manifest>